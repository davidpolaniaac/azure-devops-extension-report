{
  "name": "azure-devops-extension",
  "version": "1.0.3",
  "description": "DevOps web extension",
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --output-path dist --rev-version",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.json src/Extension/**/*.json",
    "publish-extension": "tfx extension publish --manifest-globs azure-devops-extension.json src/Extension/**/*.json"
  },
  "dependencies": {
    "azure-devops-extension-api": "^1.154.1",
    "azure-devops-extension-sdk": "^2.0.11",
    "azure-devops-ui": "^1.156.1",
    "react": "~16.3.2",
    "react-dom": "~16.3.2"
  },
  "devDependencies": {
    "@types/react": "^16.3.2",
    "@types/react-dom": "^16.0.7",
    "base64-inline-loader": "^2.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "~6.7.3",
    "file-loader": "~6.2.0",
    "node-sass": "~4.9.4",
    "rimraf": "~2.6.2",
    "sass-loader": "~13.2.0",
    "style-loader": "~3.3.1",
    "tfx-cli": "^0.6.4",
    "ts-loader": "~9.4.2",
    "typescript": "^2.9.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
