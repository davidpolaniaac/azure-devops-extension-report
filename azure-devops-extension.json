{
    "manifestVersion": 1,
    "id": "devops-extension-result-html",
    "publisher": "dpolania",
    "version": "1.0.240",
    "name": "DevOps Result - Extension",
    "description": "Publish result in formate HTML",
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "logo.png"
    },
    "scopes": ["vso.code_full"],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}